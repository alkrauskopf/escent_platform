/* tables style for jquery table sort */
table    { width:100%; border-collapse: collapse; border:1px solid #ccc; }
thead th { background-color: #e6EEEE; }
th       { font-size: 12px; line-height:2; text-align:center; border:1px solid #ccc; }
td       { font-size: 12px; line-height:2; border:1px solid #ccc; padding-left: 4px; }

table.tablesorter {
	text-align: left;
}
table.tablesorter thead tr th, table.tablesorter tfoot tr th {
	background-color: #e6EEEE;
}
table.tablesorter thead tr .header {
	background-image: url(<%= asset_path('bg.gif') %>);
	background-repeat: no-repeat;
	background-position: center right;
	cursor: pointer;
}
table.tablesorter tbody td {
	color: #3D3D3D;
	background-color: #FFF;
	vertical-align: top;
}
table.tablesorter tbody tr.odd td {
	background-color:#F0F0F6;
}
table.tablesorter thead tr .headerSortUp {
	background-image: url(<%= asset_path('asc.gif') %>);
}
table.tablesorter thead tr .headerSortDown {
	background-image: url(<%= asset_path('desc.gif') %>);
}
table.tablesorter thead tr .headerSortDown, table.tablesorter thead tr .headerSortUp {
background-color: #8dbdd8;
}

