/*
	make a admin Organization Account tag section selector much more clean
	added by victor 
*/
.section_selector { margin-bottom: 20px; line-height: 32px; }
.section_selector div a { display: block; padding: 0 15px; text-decoration: none; }
.section_selector div span { display: block; padding: 0 15px; }
.section_selector div {
	float: left;	
	height: 32px;
	background: url(<%= asset_path('section_selector_bg_h.png') %>) repeat-x top left;
}
.section_selector div.border { border-left: 1px solid #ced4e2;	}
.section_selector div.first div { background: url(<%= asset_path('section_selector_bg_first.png') %>) no-repeat top left; }
.section_selector div.first div.active { background: url(<%= asset_path('section_selector_bg_first_h.png') %>) no-repeat top left; }
.section_selector div.last div { background: url(<%= asset_path('section_selector_bg_last.png') %>) no-repeat top right; }
.section_selector div.last div.active { background: url(<%= asset_path('section_selector_bg_last_h.png') %>) no-repeat top right; }
.section_selector div#active_section { background: url(<%= asset_path('section_selector_bg.png') %>) repeat-x top left; }

