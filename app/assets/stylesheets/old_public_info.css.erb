h1 span { background-image:url(<%= asset_path('EP_logo.png') %>); }
body { background:#999 url(<%= asset_path('bg_body.jpg') %>) repeat-x left top; }
#mainContent { float: right; width: 600px; }
#member_ico { float: left; }
#member_info { float: left; margin-left: 20px; }
#member_info .name { font-size: 12px; color: #000; font-weight: bold; }
#member_info .info { padding: 20px 0; line-height: 25px; }
.title_profile img { margin-left: 197px; }

#member_ico img { border: 1px solid #adadad; }
#sideContent { float: left; margin-left: 20px; padding-top: 50px; }
input.text { width: 138px; margin-top: 2px; }
input.search_submit { width:41px; height:30px; }
input.search_submit { background:url(<%= asset_path('button_go_blue.png')%>) no-repeat top left; border: none; }
.title_find img { margin-left: 70px; }	
#sideContent { width: 225px; }
#search { padding-bottom: 10px; border-bottom: 1px dashed #999; }
.radio_warp { padding-left: 5px; }
.radio_warp input, .radio_warp label { float: left; }
.radio_warp label { width: 80px; padding-left: 5px; }
.radio_warp label.short { width: 60px; }
.keyword_warp { padding-top: 10px; padding-left: 10px; }
.title_visit img { margin: 10px 0 0 10px; }
#visit ul li { 
	background:url(<%= asset_path('bullet_grey.gif')%>) no-repeat center left;
	padding-left: 15px; font-size: 13px;
}

/* Member Profile */
.mempro_box { padding: 5px 15px 15px; margin:10px 0; }
.mempro_title { margin-bottom: 15px; }
.mempro_box .title_profile_large { margin-bottom: 15px; font-size: 14px; color: #D7452C; }
.mempro_box { font-family:Verdana, Arial, Helvetica, sans-serif; color:#636363; font-size:11px; }
.mempro_box a { color:#2176ad; }
.mempro_box .instructions { color:#2176ad; font-size:9px; font-weight:bold; }
.mempro_box .title_large_blue { text-align:left;  font-size:14px; }
.mempro_box { border:1px solid #dadada;	background-color:#FFFFFF; }

.favorite_organizations h4 { font-size: 13px; }
.favorite_organizations h4 a { color: #185D9E; text-decoration: none; border-bottom: 2px solid #87AECF; }
.favorite_organizations p { color: #626262; padding-left: 20px; }
.favorite_organizations p a,
.favorite_organizations p span.add_color { color: #185D9E; }