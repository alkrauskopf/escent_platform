<% seq = assess.sequence_score_for(ActMaster.act_std, 'ACT') -%>
<% align = assess.alignment_score_for(ActMaster.act_std, 'ACT') -%>
<table style="width:90%">
  <tbody>
    <tr>
      <td class = 'static_assess_thumbs' style="width:35%"%>
        <span class="sequence">
          Sequencing&nbsp;
          <% if seq.nil? %>
              <span class="no_thumb"></span>
          <% elsif seq < 60 -%>
              <span class="down_thumb"></span>
          <% elsif seq > 80 -%>
              <span class="up_thumb"></span>
          <% else %>
              <span class="no_thumb"></span>
          <% end -%>
        </span>
      </td>
      <td class = 'static_assess_thumbs' style="width:35%"%>
        <span class="align">
          Alignment&nbsp;
          <% if align.nil? %>
              <span class="no_thumb"></span>
          <% elsif align < 60 -%>
              <span class="down_thumb"></span>
          <% elsif align > 80 -%>
              <span class="up_thumb"></span>
          <% else %>
              <span class="no_thumb"></span>
          <% end -%>
        </span>
      </td>
      <td style="width:15%">
        <% if !assess.is_locked -%>
            <%= (image_tag "lock_no.png",:width => 15) -%>
        <% else -%>
            <%= (image_tag "lock_yes.png",:width => 15) -%>
        <% end -%>
      </td>
      <td style="width:15%">
        <% if assess.is_calibrated -%>
            <%= (image_tag "calibrated_yes.png",:width => 18) -%>
        <% end %>
      </td>
    </tr>
  </tbody>
</table>
