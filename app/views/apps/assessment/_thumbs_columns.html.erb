			<% seq_score = quest.sequence_score_for(std, filter) -%>
			<% align_score = quest.alignment_score_for(std, filter) -%>
				<td style=" width:3%; color: #000000; padding-top:5px; padding-left:0px; vertical-align:top; font-size:10px;" >
					<% if seq_score.nil? %>
						<center>--</center>
					<% elsif seq_score < 60 -%>
						<%= (image_tag "thumbs_down_right.png",:width => 25) %>
					<% elsif seq_score > 80 -%>
						<%= (image_tag "thumbs_up_right.png",:width => 25) -%>
					<% else %>
						<center>--</center>
					<% end -%>
				</td>
				<td style=" width:3%; color: #000000; padding-top:5px; padding-left:0px; vertical-align:top; font-size:10px;" >
					<% if align_score.nil? %>
						<center>--</center>
					<% elsif align_score < 50 -%>
						<%= (image_tag "thumbs_down_left.png",:width => 25) %>
					<% elsif align_score > 80 -%>
						<%= (image_tag "thumbs_up_left.png",:width => 25) -%>
					<% else %>
						<center>--</center>
					<% end -%>
				</td>