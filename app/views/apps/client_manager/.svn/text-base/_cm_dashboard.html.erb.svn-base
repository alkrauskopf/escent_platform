<div id ="cm_dashboard">

	<% unless @current_organization.active_clients.empty?-%>
		<% family = "Garamond, Time New Roman, Helvitica,sans-serif" -%>
		<table  style="width:770px; border:3px solid #ddd; margin-left:55px; font-family:<%=family%>">
			<thead>
				<tr style="border:1px solid #ddd;border-bottom:0px; ">
					<th style=" width:160px; width:fixed; border:1px solid #ddd; border-left:0px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; vertical-align:middle; text-align:center; font-weight:normal; font-size:13px" >
						<%= pluralize(@current_organization.active_clients.size, " Active Client") %>
					</th>
					<th   style=" width:160px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
						Assigned&nbsp;Staff
					</th>
					<th   style=" width:50px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
						Users
					</th>					
					<th   style=" width:50px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
						Tools
					</th>
					<th   style=" width:70px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
						Resources
					</th>					
					<th   style=" width:70px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
						Offerings
					</th>
					<th   style=" width:70px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
						STAT<br/>Cases
					</th>
					<th   style=" width:70px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
						CTL<br/>Obsrvs
					</th>
					<th   style=" width:70px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
						ELT<br/><%= EltRubric.all.collect{|r| r.abbrev}.join("/")%>
					</th>
				</tr>				
			</thead>
			<% @current_organization.active_clients.each do |client| -%>
				<tbody>
					<tr style="border:1px solid #ddd;border-bottom:0px; ">
						<td style=" width:160px; width:fixed; border:1px solid #ddd; border-left:0px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; vertical-align:middle; text-align:center; font-weight:normal; font-size:13px" >
							<%= client.medium_name %>
						</td>
						<td   style=" width:150px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
							<% if @current_organization.consultants_for_client(client).empty? -%>
								<span style="color:#C73914"><i>No Team</i></span>
							<%else%>
								<%=@current_organization.consultant_list_for_client(client)%>
							<% end -%>
						</td>
						<td   style=" width:50px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
							<%= client.users.size %>
						</td>				
						<td   style=" width:50px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
							<%= client.enabled_apps.size %>
						</td>
						<td   style=" width:70px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
							<%= client.contents.size %>
						</td>					
						<td   style=" width:70px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
							<%= client.classrooms.active.size %>
						</td>
						<td   style=" width:70px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
							<%= client.ista_cases.size %>
						</td>
						<td   style=" width:70px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
							<%= client.tlt_sessions.size %>
						</td>
						<td   style=" width:80px; width:fixed; border:1px solid #ddd; border-bottom:0px solid #ddd; color: #000000; line-height: 1.5em; padding-top:5px; padding-bottom:5px; vertical-align:middle;text-align:center; font-weight:normal; font-size:11.5px" >
							<% if client.last_elt_submitted -%>
								<%= client.last_elt_submitted.elt_case_indicators.for_rubric(EltRubric.lacking.first).size %>
								/<%= client.last_elt_submitted.elt_case_indicators.for_rubric(EltRubric.emerging.first).size %>
								/<%= client.last_elt_submitted.elt_case_indicators.for_rubric(EltRubric.proficient.first).size %>
								/<%= client.last_elt_submitted.elt_case_indicators.for_rubric(EltRubric.exemplary.first).size %>
							<% else -%>
								0/0/0/0
							<% end -%>
						</td>
					</tr>				
				</tbody>		
			<% end -%>
		</table>
	<% else -%>
	  	<div style="text-align:center;  word-wrap:break-word; color: #2061a5;  font-size: 12px; font-style: normal; font-weight: normal">
				<br/><center>There are no Active <%= @current_organization.name %>&nbsp;clients</center>
		</div>
	<% end -%>
</div>
