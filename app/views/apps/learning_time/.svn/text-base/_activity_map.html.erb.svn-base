
<div class ="activity_map">
	<table id="activity_map_<%=org.public_id%>" style="margin-top:0px; width:490px; width:fixed; border:0px solid #ddd;">
		<tbody>  
			<tr>
				<td class="resource_levels_close" style="width:480px;">
                    <%= link_to '', :controller => "apps/learning_time", :action => :show_activity_map, :org => @current_organization, :organization_id => @current_organization %>
                        <%=org.app_provider(@app).short_name %> Activity Map
                    <span></span>
				</td>
			</tr>
		</tbody>
	</table>
	<script type="text/javascript" charset="utf-8">
	jQ(document).ready(function() {
		jQ('#activity_map_<%=org.public_id%> .resource_levels_close').toggleResourceLevels();
		jQ('#activity_map_<%=org.public_id%> .resource_levels_open').toggleResourceLevels();
	});
	</script>
</div>
