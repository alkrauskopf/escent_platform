<div id="list_cases">

		<% org.elt_cases.for_cycle(cycle).last_first.each do |elt| -%>
		<% elt_label = elt.created_at.strftime("%b %d,%Y") + "  " + elt.user.last_name -%>
		<table id="resource_list_<%=elt.id%>" style="width:950px;">
			<tbody>  
				<tr>
					<td class="resource_levels_close" style="width:950px;">
	
					<%= link_to '', :controller => "apps/learning_time", :action => :show_case_summary, :organization_id => @current_organization, :school_id=>org, :elt_case_id=>elt, :app_id=> app.id  %>
					<%= elt_label %>&nbsp;<%= elt.submitted? ? ", (Submitted: " + elt.submit_date.strftime("%m-%d-%y") + ")" : ", (Draft)" %>
					<span></span>
					</td>
				</tr>
			</tbody>
		</table>
	<script type="text/javascript" charset="utf-8">
	jQ(document).ready(function() {
		jQ('#resource_list_<%=elt.id%> .resource_levels_close').toggleResourceLevels();
		jQ('#resource_list_<%=elt.id%> .resource_levels_open').toggleResourceLevels();	
	});
	</script>

		<% end -%>
</div>

