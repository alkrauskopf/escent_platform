<div id="manage_element_indicators_<%=element.public_id%>">
	<table  style="width:750px; width:fixed; border-top:0px solid #000; margin-top:10px; margin-bottom:0px; margin-left:0px">
		<tbody>  
			<tr style="background:<%= element.form_background%>;">
				<td id="refresh_table_<%=element.public_id%>" style=" width:115px; width:fixed; font-size:9px; vertical-align:bottom; cursor:pointer" >
					<span class="fsn-button ui-button ui-state-default ui-corner-all">
						Refresh
					</span>
				</td>
				<td style=" text-align:center; font-size:12px;  color:<%=element.e_font_color %>">
					<span style="font-size:9px"><%= activity.master? ? "(Master)&nbsp;&nbsp;&nbsp;&nbsp;" : ""%></span>
					<%= element.abbrev %>:&nbsp;<%=element.name%>
					<span style="font-size:9px"><%= element.active? ? "" : "&nbsp;&nbsp;&nbsp;&nbsp;(Not Active)"%></span>
				</td>
				<td  style=" width:115px; width:fixed; font-size:9px; vertical-align:bottom; cursor:pointer" >
					<% button_label = activity.master? ? " Indicator" : " Look-for" %>
					<span class="fsn-button ui-button ui-state-default ui-corner-all">
						<%= link_to "New " + button_label, {:controller => "/apps/learning_time", :action => :maintain_indicator, :app_id=> app.id, :task=>"New", :elt_type_id=>activity, :elt_element_id=>element, :organization_id => @current_organization},{:target=>"_blank"}  %>
					</span>
				</td>
			</tr>
		</tbody>
	</table>
<script type="text/javascript">
jQ(document).ready(function() {
	jQ("#refresh_table_<%=element.public_id%>").click(function() {
		var url = "<%= url_for :controller => "apps/learning_time", :action => :refresh_element_indicator, :organization_id => @current_organization, :elt_element_id => element, :elt_type_id=> activity, :app_id=> app.id %>";
		jQ.get(url,{},  function(data) {
				jQ("#manage_element_indicators_<%=element.public_id%>").html(data);
				jQ("#manage_element_indicators_<%=element.public_id%>").show();
					});
	})
});
</script>
	<table  style="width:750px; width:fixed; border:2px solid <%= element.i_form_background%>; margin-top:0px; margin-bottom:20px; margin-left:0px">
		<tbody>  
			<% element.elt_indicators.for_activity(activity).all.each do |ind| -%>
				<tr style="border:1px solid #ddd; background:<%= element.i_form_background%>;">
					<td  style="width:20px;width:fixed; border:0px solid #ddd; font-size:11px; padding-top:4px; text-align:center; vertical-align:top; color:#000000; ">
						<%= ind.position %>)
					</td>
					<% button_label = ind.active? ? "button_enabled.png" : "button_disabled.png"-%>
					<td id="toggle_active_<%= ind.public_id %>" style=" border:0px solid #ddd; width:10px;  width:fixed; cursor:pointer; padding-left:0px; padding-top:8px; vertical-align:top" >
						<%= (image_tag button_label,:width => 15) %>
					</td>
					<td  style="width:35px; width:fixed; border:0px solid #ddd; padding-top:5px; padding-left:10px;">
						<span class="fsn-button ui-button ui-state-default ui-corner-all">
							<%= link_to 'Edit', {:controller => "/apps/learning_time", :action => :maintain_indicator, :app_id=> app.id, :task=>"Update", :elt_type_id=>activity, :elt_element_id=>element, :elt_indicator_id=>ind, :organization_id => @current_organization},{:target=>"_blank"}  %>
						</span>
					</td>
					<td style=" text-align:left; vertical-align:top, font-size:11px; padding-bottom:10px; :font-weight:normal; line-height:1.2em; color:#000000">
						<%=ind.indicator%>
						<% unless ind.elt_indicator_lookfors.empty? -%>
							<% ind.elt_indicator_lookfors.all.each do |lf| -%>						
								<div class = "app_elt_lookfor" style="padding-left:10px;">
									<br/><%= simple_format lf.lookfor %>
								</div>
							<% end -%>
						<% end %>
					</td>
					<td  style="width:200px;width:fixed; border:0px solid #ddd; font-size:10px; padding-top:8px; text-align:center; vertical-align:top; color: #2061a5 ; font-weight:bold">
						<%= ind.support_indicators.empty? ? "" : ind.support_indicators.collect{|si| (si.elt_element.abbrev + "-" + si.position.to_s)}.join(", ") %>
					</td>
				</tr>

<script type="text/javascript">
jQ(document).ready(function() {
	jQ("#toggle_active_<%= ind.public_id %>").click(function() {
		var url = "<%= url_for :controller => "apps/learning_time", :action => :toggle_active_indicator, :organization_id => @current_organization, :elt_indicator_id => ind, :elt_element_id => element, :elt_type_id=> activity, :app_id=> app.id %>";
		jQ.get(url,{},  function(data) {
				jQ("#manage_element_indicators_<%=element.public_id%>").html(data);
				jQ("#manage_element_indicators_<%=element.public_id%>").show();
					});
	})
});
</script>
			<% end -%>
		</tbody>
	</table>
</div>








