						<a id="<%= indicator.id%>_<%=help_tag%>" href=""><%= indicator.ind_num %></a>
							<div id="<%= indicator.id%>_<%=help_tag%>-dialog" title= "<%= indicator.elt_element.name %>" style="text-align:left;">
							    <div class="normal_text" style=" font-size:12px;">
									<table  style="width:450px; margin-left:15px;border:0px solid #ddd;"  >
										<tbody>  
											<style type="text/css" media="screen">
									    		table,tr,td { border:none }
												table tr:hover{
								        		background-color: transparent;
								      			}
								   			</style>
											<tr  style="border-top:0px solid #ddd; ">
												<td   style="border:0px solid #ddd; line-height:1.2em; font-size:13px; color:#2061a5; padding-bottom:15px; text-align:left; vertical-align:middle;">
													<%= simple_format indicator.indicator %><br/>
													<span style = "color:#CC0000; line-height:1.5em; font-size:16px; font-weight:normal"> <%= rubric.name.upcase %></span>&nbsp;&nbsp;<%= school.name %>
												</td>
											</tr>
										</tbody>
									</table>
							    </div>
							</div>
<script type="text/javascript" charset="utf-8">
    	jQ(document).ready(function() {

    	    jQ("#<%= indicator.id%>_<%=help_tag%>-dialog").dialog({
    	            bgiframe: true,
    	            autoOpen: false,
    	            width: 500,
    	            height: 210,
    	            modal: true,
    				buttons: {
    					 Close: function(){
    		                        jQ(this).dialog('close');
    		                    }
    				}
        	    }
    		);					
			
    		jQ('#<%= indicator.id%>_<%=help_tag%>').click(function(event){
    		    jQ("#<%= indicator.id%>_<%=help_tag%>-dialog").dialog('open');
    			 event.preventDefault();
    		})							

    });
</script>



