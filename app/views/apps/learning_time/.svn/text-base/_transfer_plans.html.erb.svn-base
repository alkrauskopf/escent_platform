<div id="transfer_plans_<%=org.public_id%>">
	<% if org.appl_owner?(app) -%>

		<div>
			<table style="width:750px;margin-left:0px; border:0px solid #ddd;">
				<tbody>  
					<tr  style="border:0px solid #ddd;" >
						<td  id='<%=org.id%>_plan_xfer' style="width:200px; width:fixed; color:#000000; font-weight:normal; font-size:12px; padding-left:40px; padding-bottom:0px;">
							<%=  select(:plan_org, org.id, (org.provided_app_orgs(app,false)).sort_by{|o| o.name}.collect{|o| [truncate(o.medium_name, :omission => "...", :length => 18), o.public_id]}, :prompt => "Organization With Plan") %>
						</td>
					</tr>
				</tbody>
			</table>	
		</div>

<script type="text/javascript">
jQ(document).ready(function() {
	jQ("#<%=org.id%>_plan_xfer").change(function() {
		var base_org_id = jQ("#plan_org_<%=org.id%>").val();
		var url = "<%= url_for :controller => "/apps/learning_time", :action => :transfer_plan_org, :organization_id => @current_organization, :org_id => org, :app_id=> app %>";
			jQ.get(url, {'base_org_id':base_org_id}, function(data) {
			jQ("#transfer_plans_<%=org.public_id%>").html(data);
			jQ("#transfer_plans_<%=org.public_id%>").show();
			});
	})
});
</script>
        <div style = "margin-top:20px;">
            <% unless base_org.nil? -%>
                <table style="width:700px;margin-left:0px; border:0px solid #ddd;">
                    <tbody>
                        <tr  colspan=2 style="border:0px solid #ddd;" >
                            <td  style=" color: #C73914;  font-weight:bold; font-size:12px; padding-left:0px; padding-bottom:0px;">
                                <%= base_org.name  %>
                            </td>
                        </tr>
                        <tr  style="border:0px solid #ddd;" >
                            <td  style="width:350px; width:fixed; color:#000000; font-weight:normal; font-size:12px; padding-left:0px; padding-bottom:0px;">
                                &nbsp;
                            </td>
                            <td  style="width:350px; width:fixed; color:#000000; font-weight:normal; font-size:12px; padding-left:0px; padding-bottom:0px;">
                                <%=  select(:plan_cycle, org.id, (org.elt_cycles.all).sort_by{|c| c.name}.collect{|c| [truncate(c.name, :omission => "...", :length => 18), c.id]}, :prompt => "Transfer To Cycle") %>
                            </td>
                        </tr>
                    </tbody>
                </table>
                <table  style="width:750px; width:fixed; border-top:0px solid #000; margin-top:10px; margin-bottom:0px; margin-left:0px">
                    <tbody>
                        <% base_org.elt_plans.for_provider(org).each do |elt_plan| %>
                            <tr style="border:1px solid #ddd;">
                                <td id="xfer_<%= elt_plan.public_id %>" style=" width:100px; width:fixed; text-align:left; color: #2061a5;; font-size:11px; font-weight:normal; ">
                                    <span class="fsn-button ui-button ui-state-default ui-corner-all" style="cursor:pointer;padding-left:5px;">
                                        Transfer
                                    </span>
                                </td>
                                <td style=" width:150px; width:fixed; text-align:left; color: #2061a5; line-height:1.3em; font-size:11px; font-weight:normal; ">
                                    <%= elt_plan.created_at.strftime("%m-%d-%y") %>
                                </td>
                                <td style="  text-align:left; color: #2061a5; line-height:1.3em; font-size:12px; font-weight:normal; ">
                                    <strong><%=elt_plan.open? ? "Open" : "Closed"%></strong><br/>
                                    <%= elt_plan.elt_cycle.name %>
                                </td>
                            </tr>
    <script type="text/javascript">
    jQ(document).ready(function() {
    jQ("#xfer_<%= elt_plan.public_id %>").click(function() {
        var xfer_cycle_id = jQ('#plan_cycle_<%=org.id%>').val();
        var url = "<%= url_for :controller => "/apps/learning_time", :action => :transfer_plan_org, :organization_id => @current_organization, :org_id => org, :base_org_id => base_org, :elt_plan_id => elt_plan, :app_id=> app %>";
        jQ.get(url,{'xfer_cycle_id':xfer_cycle_id},  function(data) {
            jQ("#transfer_plans_<%=org.public_id%>").html(data);
            jQ("#transfer_plans_<%=org.public_id%>").show();
                    });
    })
    });
    </script>
                        <% end -%>
                    </tbody>
                </table>
            <% end -%>
        </div>
	<% end -%>
</div>
