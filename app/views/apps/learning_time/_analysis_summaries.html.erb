
<div id ="analysis_summaries" style = "font-size:12px; color: #2061a5; padding-left:0px;">
	<div style ="text-align:center; color: #2061a5; font-size:14px; margin-top:5px; font-weight:normal">
		<%= @current_application.app_name(:provider => @current_provider) %>&nbsp;Analysis & Summary Reports
	</div>	
	<div>
		<table style="margin-left:150px; font-size:16px; font-weight:bold;  color: #2061a5; width:600px">
			<tbody >
				<tr>
					<td  style="width:40px; vertical-align:middle; text-align:left;padding-bottom:10px; padding-top:10px; padding-left:20px">
						<span class="fsn-button ui-button ui-state-default ui-corner-all">
							 <%= link_to 'Go', elt_report_path(:organization_id => @current_organization, :user_id => @current_user, :report_id => "SD"), {:target => "_blank"} %>
						</span>
					</td>
					<td style="width:550px; vertical-align:middle; text-align:left;padding-left:10px">
							 School Diagnostics & Action Plans
					</td>
				</tr>
        <% if false %>
				<tr>
					<td  style="width:40px; vertical-align:middle; text-align:left;padding-bottom:10px; padding-top:10px; padding-left:20px">
