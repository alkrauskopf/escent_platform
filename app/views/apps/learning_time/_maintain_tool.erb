    <table id="case_xfers_<%=org.public_id%>" style="margin-left:80px; width:90%; border:0px solid #ddd;">
      <tbody>
        <tr>
          <td class="resource_levels_close" style="width:95%;">
            <%= link_to '', elt_config_cases_xfer_path(:organization_id => @current_organization) %>
            Move Cases To Other Schools or Cycles
            <span></span>
          </td>
        </tr>
      </tbody>
    </table>
    <script type="text/javascript" charset="utf-8">
        jQ(document).ready(function() {
            jQ('#case_xfers_<%=org.public_id%> .resource_levels_close').toggleResourceLevels();
            jQ('#case_xfers_<%=org.public_id%> .resource_levels_open').toggleResourceLevels();
        });
    </script>
    <table id="plan_xfers_<%=org.public_id%>" style="margin-left:80px; width:90%; border:0px solid #ddd;">
      <tbody>
        <tr>
          <td class="resource_levels_close" style="width:95%;">
            <%= link_to '', elt_config_plans_xfer_path(:organization_id => @current_organization) %>
            Move A School Plan  To Another Cycle
            <span></span>
          </td>
        </tr>
      </tbody>
    </table>
    <script type="text/javascript" charset="utf-8">
        jQ(document).ready(function() {
            jQ('#plan_xfers_<%=org.public_id%> .resource_levels_close').toggleResourceLevels();
            jQ('#plan_xfers_<%=org.public_id%> .resource_levels_open').toggleResourceLevels();
        });
    </script>
  </div>
</div>
