            <span></span>
          </td>
        </tr>
      </tbody>
    </table>
    <script type="text/javascript" charset="utf-8">
    jQ(document).ready(function() {
        jQ('#plan_<%#=org.public_id%> .resource_levels_close').toggleResourceLevels();
