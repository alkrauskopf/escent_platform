<div id="mainContent" style=" border:1px solid #ddd;">
  <div id="diagnostic_box" >
    <div style="text-align:center;  word-wrap:break-word; color: #2061a5;  font-size: 15px; font-style: normal; font-weight: normal">
      <%= elt_case.elt_cycle ? elt_case.elt_cycle.name: "Removed Diagnostic Cycle" -%>:&nbsp;<%= elt_case.name %>
    </div>
    <div style="text-align:center;  word-wrap:break-word; color: #2061a5;  font-size: 13px; font-style: normal; font-weight: normal">
      <%= elt_case.activity.collect_subject? ? (elt_case.subject_area ? (elt_case.subject_area.name ) : 'No Subject') : '' -%>
      <%= elt_case.activity.collect_grade? ? ( elt_case.grade_level ? " " + elt_case.grade_level.name : "  No Grade Level") : '' -%>
    </div>
  <% unless elt_case.elt_cycle.nil? %>
    <%= render :partial => '/apps/learning_time/form_case_b', :locals=> {:elt_case => elt_case} %>
  <% end -%>
  </div>
</div>
