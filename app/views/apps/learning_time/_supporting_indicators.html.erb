
<div id ="supporting_<%=std.public_id%>" style = "font-size:12px; color: #2061a5; padding-left:0px;">
	<table id="indicator_<%=std.public_id%>" style="margin-top:0px; width:490px; width:fixed; border:0px solid #ddd;">
		<tbody>  
			<tr>
				<td class="resource_levels_close" style="width:480px;">
  				<%#= link_to '', :controller => "apps/learning_time", :action => :list_case_evidences, :organization_id => @current_organization, :elt_case_id => elt_case, :elt_std_indicator_id => std  %>
          <%= link_to '', elt_case_support_findings_path(:organization_id => @current_organization, :org_id => elt_case.organization,:elt_cycle_id=> elt_case.elt_cycle, :elt_std_indicator_id => std)  %>
				<%=std.description %>
				<span></span>
				</td>
			</tr>
		</tbody>
	</table>
	<script type="text/javascript" charset="utf-8">
	jQ(document).ready(function() {
		jQ('#indicator_<%=std.public_id%> .resource_levels_close').toggleResourceLevels();
		jQ('#indicator_<%=std.public_id%> .resource_levels_open').toggleResourceLevels();
	});
	</script>
</div>
