
<div id ="supporting_comments<%=element.public_id%>" style = "font-size:12px; color: #2061a5; padding-left:0px;">
	<table id="comments_<%=element.public_id%>" style="margin-top:0px; width:490px; width:fixed; border:0px solid #ddd;">
		<tbody>  
			<tr>
				<td class="resource_levels_close" style="width:480px;">

                    <%= link_to '', :controller => "apps/learning_time", :action => :list_case_comments, :organization_id => @current_organization, :elt_case_id => elt_case, :elt_element_id => element  %>
                 
                    Supporting Comments
                    <span></span>
				</td>
			</tr>
		</tbody>
	</table>
	<script type="text/javascript" charset="utf-8">
	jQ(document).ready(function() {
		jQ('#comments_<%=element.public_id%> .resource_levels_close').toggleResourceLevels();
		jQ('#comments_<%=element.public_id%> .resource_levels_open').toggleResourceLevels();
	});
	</script>
</div>
