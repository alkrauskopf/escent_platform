<div class='container'>
  <div class= 'panel_title_blue'>
    <%= @element.name %>&nbsp;Indicator
  </div>
  <%= render :partial => "/shared/error_and_notice" %>
  <div class="Form_Container clear">
    <span class="fsn-button ui-button ui-state-default ui-corner-all" style ='margin:20px'>
      <%= link_to 'Return', elt_config_standard_element_edit_path(:elt_element_id => @element, :organization_id => @current_organization) %>
    </span>
    <%= form_for(:elt_std_indicator, :url => elt_config_standard_element_indicator_update_path(:organization_id => @current_organization, :elt_std_indicator_id => @indicator)) do |f| %>
        <%= render :partial => "/apps/learning_time_standards/form_indicator" %>
        <div class="Form_Row">
          <div class="Form_Row_Title">
          </div>
          <div class="Form_Row_Data">
            <%= f.submit :class =>"fsn-button ui-button ui-state-default ui-corner-all save" %>
          </div>
        </div>
    <% end -%>
  </div>
  <%= render :partial => "apps/learning_time_standards/indicator_descriptors", :locals=>{:indicator => @indicator} %>
</div>