<div class ='container'>
  <div class= 'panel_title_blue'>
    <%= @element.name %>&nbsp;Indicator
  </div>
  <%= render :partial => "/shared/error_and_notice" %>
  <div class="Form_Container clear">
    <%= form_for(:elt_std_indicator, :url => elt_config_standard_element_indicator_add_path(:organization_id => @current_organization, :elt_element_id=>@element, :elt_std_indicator_id => @indicator)) do |f| %>
        <%= render :partial => "/apps/learning_time_standards/form_indicator" %>
        <div class="Form_Row">
          <div class="Form_Row_Title">
          </div>
          <div class="Form_Row_Data">
            <%= f.submit :class =>"fsn-button ui-button ui-state-default ui-corner-all save" %>
          </div>
        </div>
    <% end -%>
  </div>
</div>