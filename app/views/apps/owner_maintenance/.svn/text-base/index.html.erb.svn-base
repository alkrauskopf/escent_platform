<div id="main_content" >
	<div style ="color: #C83A14; font-size:18px; margin-top:5px; font-weight:bold"> 
		<br/>
		<center>
			<strong><%=  @app.name %></strong><br/>
			<span style="font-size:13px; padding-top:10px; font-weight:normal"><u>Universal</u> changes to <%= @app.abbrev %>&nbsp;are made here.
			</span>
		</center>
	</div>
		<% if (@app.ctl?) -%>
			<%= render :partial => "/apps/owner_maintenance/owner_maintenance_ctl", :locals => {:app => @app}  %>
			<% elsif (@app.pd?) -%>
				<%= render :partial => "/apps/owner_maintenance/owner_maintenance_pd", :locals => {:app => @app}  %>
				<% elsif (@app.classroom?) -%>
					<%= render :partial => "/apps/owner_maintenance/owner_maintenance_clsrm", :locals => {:app => @app}  %>
					<% elsif (@app.ifa?) -%>
						<%= render :partial => "/apps/owner_maintenance/owner_maintenance_ifa", :locals => {:app => @app}  %>
						<% elsif (@app.core?) -%>
							<%= render :partial => "/apps/owner_maintenance/owner_maintenance_core", :locals => {:app => @app}  %>
							<% elsif (@app.elt?) -%>
								<%= render :partial => "/apps/owner_maintenance/owner_maintenance_elt", :locals => {:app => @app}  %>		
		<% else -%>
			<span style="color: #C83A14; font-size:12px;  font-weight:normal">
				<br/></br/><center>
					No Maintenance App For <%= @app.abbrev %>.
				</center><br/></br/>
			</span>	
		<% end -%>
	<br/>
</div>