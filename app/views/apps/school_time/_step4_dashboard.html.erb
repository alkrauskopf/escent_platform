
	<% group = IstaGroup.electives[0] -%>
	<% grp_back = "#" + group.background_2 -%>
	<tr style="border:1px solid #ddd; background:<%=grp_back%>;">
		<td style=" width:60px; width:fixed; border:1px solid #ddd; border-right:0px solid #ddd; color: #000000; vertical-align:middle; text-align:left; font-weight:bold; font-size:12px;"  >
				<%=group.name%>
		</td>
		<td style=" width:140px; width:fixed; border:1px solid #ddd; border-left:0px solid #ddd; color: #000000; vertical-align:middle; float:right; font-weight:normal; font-size:12px;"  >
			Total Redirected&nbsp;&nbsp;hrs/yr
		</td>
		<%columns.each do |idx| -%>	
			<td   style=" width:160px; width:fixed; border:1px solid #ddd; color: #000000; line-height: 1.2em; padding-left:3px;vertical-align:middle;text-align:center; font-weight:normal; font-size:12px" >
				<% unless !cases[idx] -%>
					<%=  cases[idx].electives_hours_year_redirected -%>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= number_to_percentage((cases[idx].electives_hours_year_redirected_pct).round, :precision => 0) %>
				<% end -%>
			</td>
		<% end -%>
	</tr>
	<tr style="border:1px solid #ddd; background:<%=grp_back%>;">
		<td style=" width:60px; width:fixed; border:1px solid #ddd; border-right:0px solid #ddd; color: #000000; vertical-align:middle; text-align:left; font-weight:bold; font-size:12px;"  >
			&nbsp;
		</td>
		<td style=" width:140px; width:fixed; border:1px solid #ddd; border-left:0px solid #ddd; color: #000000; vertical-align:top; text-align:right; font-weight:normal; font-size:12px;"  >
				Est. annual Time
		</td>
		<%columns.each do |idx| -%>	
			<td   style=" width:160px; width:fixed; border:1px solid #ddd; color:  #000000; line-height: 1.5em; padding-left:3px;vertical-align:middle;text-align:center; font-weight:normal; font-size:12px" >
				<% unless !cases[idx] -%>
