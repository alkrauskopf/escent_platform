	<table  style="width:645px; margin-left:0px;padding-top:0px; border:0px solid #ddd;">
		<tbody>  
			<style type="text/css" media="screen">
	    		table,tr,td { border:none }
				table tr:hover{
        		background-color: transparent;
      			}
   			</style>
			<% if session.irr?%>
				<tr style=" border:0px solid #ddd;" >
					<td colspan=3 style="border:0x solid #ddd; vertical-align:top;  padding-top:0px; font-size:9px; text-align:center;">
						<center><%=image_tag("black_belt.png", :width => 50)%></center>
					</td>
				</tr>
			<% end -%>
			<tr style=" border:0px solid #ddd;" >
				<td colspan=3 style="border:0x solid #ddd; line-height:1.5em; vertical-align:top;  padding-bottom:10px; font-size:16px; text-align:center; font-weight:bold; color:#C73914;">
					<%= session.classroom_name %>,&nbsp;<%= session.class_period_name %></br/>
					<span style = "font-size:12px; font-weight:normal; line-height:1.2em;">
						<%= session.session_date.strftime("%b %d, %Y") %>
						<% if session.training? %>Practice					
						<% elsif !session.students == 0 %>
							,&nbsp;<%= pluralize(session.students, "Student")%>								
						<% end -%>
						<br/>
						Class Time:&nbsp;<%= pluralize(number_with_precision(session.duration/60.0, :precision => 0), " Minute") %>	
						,&nbsp;&nbsp;Observation Time:&nbsp;<%= pluralize(number_with_precision(session.session_length/60.0, :precision => 1), " Minute") %>											
					</span>
				</td>
			</tr>
			<% if session.topic %>
				<tr  style="border:0px solid #ddd;">
					<td   style="width:105px; border:0px solid #ddd; padding-right:0px; text-align:right; color:#000000;">
						Unit:		
					</td>
					<td   style="border:0px solid #ddd;  padding-left:10px; text-align:left; color:#000000;">
						<%=  session.topic.title.titleize %>				
					</td>
				</tr>		
			<% end -%>
			<% unless session.lesson.empty? %>
				<tr  style="border:0px solid #ddd;">
					<td   style="width:105px; border:0px solid #ddd; padding-right:0px; text-align:right; color:#000000;">
						Lesson:			
					</td>
					<td   style="border:0px solid #ddd;  padding-left:10px; text-align:left; color:#000000;">
						<%=  session.lesson.titleize %>				
					</td>
				</tr>
			<% end -%>
		</tbody>
	</table>

<script type="text/javascript">
jQ(document).ready(function() {
	jQ('a.task_button').click(function() {
		var $this = jQ(this);
		var url = $this.attr("href");
			jQ.get(url, function(data){
				jQ("#static_heading").html(data);
				jQ("#static_heading").show();
			})
			return false;
	})
});
</script>

<script type="text/javascript">
	jQ(document).ready(function() {
  		  jQ("#pre_header_<%= session.public_id %>").show();
		  jQ("#pre_descript_<%= session.public_id %>").hide();		  

	jQ("#pre_show_<%= session.public_id %>").click(function() {
		  	jQ("#pre_header_<%= session.public_id %>").slideUp("slow");	 
	   		jQ("#pre_descript_<%= session.public_id %>").slideDown("slow");
	})

	jQ("#pre_hide_<%= session.public_id %>").click(function() {
	   		jQ("#pre_descript_<%= session.public_id %>").slideUp("slow");
	   		jQ("#pre_header_<%= session.public_id %>").show();
	})	
});
</script>

