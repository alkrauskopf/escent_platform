<div id="backlog_lists" style = "font-size:12px; color: #2061a5; padding-left:10px;">
	<div style ="color: #2061a5; font-size:14px; margin-top:5px; font-weight:bold"> 
		<strong><center>Pending Post-Conferences</center></strong>
	</div>
	
		<table id="observer_back_list"   style="width:850px; ">
			<tbody>  
				<tr>
					<td class="resource_levels_close">
						<%= link_to '', :controller => "apps/time_learning", :action => :subject_tlt_sessions, :organization_id => @current_organization,:app_id=> app.id, :group => "observer", :user_id => @current_user, :tab=>"open"   %>
						<%= @current_user.last_name%>&nbsp;=> As Observer
						<span></span>
					</td>
				</tr>
			</tbody>
		</table>
	<script type="text/javascript" charset="utf-8">
	jQ(document).ready(function() {
		jQ('#observer_back_list .resource_levels_close').toggleResourceLevels();
		jQ('#observer_back_list .resource_levels_open').toggleResourceLevels();	
	});
	</script>
	
		<table id="teacher_back_list"   style="width:850px; ">
			<tbody>  
				<tr>
					<td class="resource_levels_close">
						<%= link_to '', :controller => "apps/time_learning", :action => :subject_tlt_sessions, :organization_id => @current_organization,:app_id=> app.id, :group => "teacher", :user_id => @current_user, :tab=>"open"   %>
						<%= @current_user.last_name%>&nbsp;=> As Teacher
						<span></span>
					</td>
				</tr>
			</tbody>
		</table>
	<script type="text/javascript" charset="utf-8">
	jQ(document).ready(function() {
		jQ('#teacher_back_list .resource_levels_close').toggleResourceLevels();
		jQ('#teacher_back_list .resource_levels_open').toggleResourceLevels();	
	});
	</script>
</div>
