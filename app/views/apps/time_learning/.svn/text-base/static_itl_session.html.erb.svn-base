	<br style="clear:both">
	<table  style="width:925px; border:0px solid #ddd;padding-top:0px;">
		<tbody>  
			<style type="text/css" media="screen">
	    		table,tr,td { border:none }
				table tr:hover{
        		background-color: transparent;
      			}
   			</style>
			<tr>
				<td style= "width:230px; border:0px solid #ddd; padding-bottom:30px; padding-left:0px;">
					<%= render (:partial => "itl_left_header", :locals => {:session => @tlt_session, :observer => @observer_update, :teacher => @teacher_update, :survey=>@survey}) %>	
				</td>
				<td style= "width:650px; border:0px solid #ddd; vertical-align:top; padding-bottom:30px; padding-left:20px;">
					<%= render (:partial => "itl_right_header", :locals => {:session => @tlt_session, :observer => @observer_update, :teacher => @teacher_update}) %>	
					<%= render (:partial => "itl_session_notes", :locals => {:session => @tlt_session, :observer => @observer_update, :teacher => @teacher_update}) %>	
					<%= render (:partial => "video_attach", :locals => {:session => @tlt_session, :teacher => @teacher_update, :observer => @observer_update}) %>	
					<% unless @tlt_session.training? || @tlt_session.irr? -%>
						<%= render (:partial => "itl_session_student_survey", :locals => {:session => @tlt_session, :observer => @observer_update}) %>	
					<% end -%>
					<% if @tlt_session.app_methods.include?(AppMethod.rs.first)%>
						<%= render (:partial => "research", :locals => {:session => @tlt_session, :irr=> false, :label => "Observation Graphs"}) %>	
					<% end -%>
					<% if @tlt_session.blackbelt_available? %>
						<%= render (:partial => "research", :locals => {:session => @tlt_session.irr_session, :irr=> true, :label => "Calibration Graphs"}) %>	
					<% end -%>
	
					<div style="margin-left:20px; padding-top:10px; padding-bottom:10px;">
						<%= render :partial => "itl_activity_summary", :locals => {:session => @tlt_session, :update=>@observer_update} %>	
					</div>


					<table id="<%=@tlt_session.public_id %>_log"   style="width:650px;  color:#000000;">
						<tbody>  
							<tr>
								<td class="resource_levels_close">
				
								<%= link_to '', :controller => "apps/time_learning", :action => :log_summary, :organization_id => @current_organization, :tlt_session_id => @tlt_session, :update=> @observer_update %>
								Log&nbsp;&nbsp;(<%= pluralize(@tlt_session.tlt_session_logs.size, " Observation")%>)
								<span></span>
								</td>
							</tr>
						</tbody>
					</table>
				<script type="text/javascript" charset="utf-8">
				jQ(document).ready(function() {
					jQ('#<%=@tlt_session.public_id %>_log .resource_levels_close').toggleResourceLevels();
					jQ('#<%=@tlt_session.public_id %>_log .resource_levels_open').toggleResourceLevels();	
				});
				</script>
				</td>
				<td style= "width:45px; width:fixed; border:0px solid #ddd; padding-left:0px;">
					&nbsp;&nbsp;&nbsp;
				</td>	
			</tr>
		</tbody>
	</table>

