<div id="backlog_lists" style = "font-size:12px; color: #2061a5; padding-left:10px;">
	<div style ="text-align:center; color: #2061a5; font-size:14px; margin-top:5px; font-weight:bold">
		<strong>Pending Post-Conferences</strong>
	</div>
		<table id="observer_back_list"   style="width:850px; ">
			<tbody>  
				<tr>
					<td class="resource_levels_close">
						<%= link_to '', ctl_backlog_path(:organization_id => @current_organization, :group => 'observer', :user_id => @current_user, :tab=>'open') %>
						<%= @current_user.last_name%>&nbsp;=> As Observer
						<span></span>
					</td>
				</tr>
			</tbody>
		</table>
	<script type="text/javascript" charset="utf-8">
	jQ(document).ready(function() {
		jQ('#observer_back_list .resource_levels_close').toggleResourceLevels();
		jQ('#observer_back_list .resource_levels_open').toggleResourceLevels();	
	});
	</script>
	
		<table id="teacher_back_list"   style="width:850px; ">
			<tbody>  
				<tr>
					<td class="resource_levels_close">
						<%= link_to '', ctl_backlog_path(:organization_id => @current_organization, :group => 'teacher', :user_id => @current_user, :tab=>'open') %>
						<%= @current_user.last_name%>&nbsp;=> As Teacher
						<span></span>
					</td>
				</tr>
			</tbody>
		</table>
	<script type="text/javascript" charset="utf-8">
	jQ(document).ready(function() {
		jQ('#teacher_back_list .resource_levels_close').toggleResourceLevels();
		jQ('#teacher_back_list .resource_levels_open').toggleResourceLevels();	
	});
	</script>
</div>
