<div id="entity_dashboard_<%= dashboard.public_id %>">
  <% if !@dashboard.nil? %>
    <div class="row dashboard_title_1">
      <div class="col-md-1"></div>
      <div class="col-md-10 ">
        <%= @dashboard['id'] %> | <%= @dashboard['name'] %> | <%= @dashboard['subject'].name %> | <%= @dashboard['period_end'].strftime("%B %Y")%>
      </div>
      <div class="col-md-1"></div>
    </div>
    <div class="row dashboard_title_2">
      <div class="col-md-1"></div>
      <div class="col-md-10 ">
        <%= pluralize(@dashboard['assess_count'], ' Assessment') %> |
        <%= pluralize(@dashboard['answer_count'], ' Answer Choice') %> |
        <%= @dashboard['proficiency']%>% Proficiency |
        <%= @dashboard['efficiency']%> Secs/Point
      </div>
      <div class="col-md-1"></div>
    </div>
      <div class="row dashboard_title_2">
        <div class="col-md-1"></div>
        <div class="col-md-10 ">
          Mastery Level: <%= @dashboard['standard'].abbrev %>&nbsp;<%= @dashboard['standard_score']%>
          <%= @dashboard['sat_score'].nil? ? '' : (', SAT ' + @dashboard['sat_score'].to_s) %>
        </div>
        <div class="col-md-1"></div>
      </div>
  <% end %>
  <% if !@assessment.nil? %>
      <div class="row ">
        <div class="col-md-2">
          <% if @assessment['comment'] != '' %>
              <div class="row dashboard_comment_header ">
                Student Comment
              </div>
              <div class="row dashboard_comment ">
                <%= @assessment['comment']%>
              </div>
          <% end %>
        </div>
        <div class="col-md-8 ">
          <div class="row dashboard_title_3">
            <%= @assessment['subject'].name %> Assessment Taken <%= time_ago_in_words(@assessment['taken_time']).sub(/about /, '') + " ago" %>
          </div>
          <div class="row dashboard_title_3">
            <%= pluralize(@assessment['answer_count'], ' Answer Choice') %> |
            <%= pluralize(number_with_precision(@assessment['total_points'], :precision => 0), ' Point') %> |
            <%= pluralize(number_with_precision(@assessment['duration_minutes'], :precision => 1), ' Minute') %> |
            <%= @assessment['proficiency']%>% | <%= @assessment['efficiency']%> Secs/Point
          </div>
          <div class="row dashboard_title_4">
            <%=@assessment['score_status']%>Mastery Level: <%= @assessment['standard'].abbrev %>&nbsp;<%= @assessment['standard_score']%>
            <%= @assessment['sat_score'].nil? ? '' : (', SAT ' + @assessment['sat_score'].to_s) %>
          </div>
        </div>
        <div class="col-md-2 dashboard_teacher">
          <div class="row">
            <%= @assessment['teacher']%>
          </div>
          <div class="row">
            <%= @assessment['status'] %>
          </div>
        </div>
      </div>
  <% end %>
  <div class="row dashboard_box">
    <% strands = standard.strands(subject) %>
    <% level_width = 14 %>
    <% strnd_width = ((100 - level_width)/strands.size).to_f.to_i.to_s + '%'%>
    <div class="row dashboard_bench_label">
      View Benchmarks, Click on Cell
    </div>
    <table >
      <tbody >
      <tr>
        <td class="dashboard_achieve_label">
          <% if !cell_milestones.empty? %>
            <i class="star_achievement"></i> &nbsp; Mastered<br/>
            <i class="wrench_work"></i> &nbsp; Work Focus
          <% end %>
        </td>
        <% strands.each do |s|%>
            <td class=" dashboard_strand_header tip_std" title="<center><%= s.name.titleize %></center>"  style="width:<%=strnd_width%>; background:<%= s.strand_background%>; color:<%= s.strand_font%>"  >
              <%= s.abbrev.upcase %>
            </td>
        <% end -%>
      </tr>
      <% standard.mastery_levels(subject).each do |ml|%>
          <tr>
            <td class="dashboard_levels" style="background:<%= ml.score_background%>; color:<%= ml.score_font%>">
              <%= mastery_label_br(ml, true).html_safe %>
            </td>
            <% standard.strands(subject).each do |s|%>
                <td class="dashboard_cells"  style="background:<%=cell_color[hash_key(ml,s)]%>; color:<%= cell_font[hash_key(ml,s)]%>; " >
                  <% if cell_milestones[hash_key(ml,s)]%>
                      <div class="dashboard_milestone"><i class="wrench_work"></i></div>
                  <% end %>
                  <% if cell_achieves[hash_key(ml,s)]%>
                      <div class="dashboard_milestone"><i class="star_achievement"></i></div>
                  <% end %>
                  <a id="b_<%= dashboard.public_id %><%= ml.id.to_s %><%= s.id.to_s %>" href="">
                    <%= cell_totals[hash_key(ml,s)] > 0 ? (cell_corrects[hash_key(ml,s)].to_s + ' of ' + cell_totals[hash_key(ml,s)].to_s + '<br/>' +
                        number_with_precision(cell_pcts[hash_key(ml,s)], :precision => 0) + '%').html_safe : ' _ '%>
                  </a>
                  <%= render :partial => "/apps/shared/help_box_ifa_benchmarks", :locals => {:id => ('b_' + dashboard.public_id + ml.id.to_s + s.id.to_s),
                                                                                             :titl=> (mastery_label(ml, true) + " | " + s.name),
                                                                                             :blue=> 'Benchmark',
                                                                                             :body=> benchmarks(s, ml, @current_standard), :height=> 400, :width=> 650 } %>

            <% end %>
          </tr>
      <% end %>
      </tbody>
    </table>

  </div>
</div>
<script>
  jQ(function(){
    jQ('td.tip_cell_<%#= div_key %>').qtip({
      content: { text: false },
      position: {
        corner: {
          target: 'rightTop',
          tooltip: 'leftTop'
        }
      },
      style: {
        background: '#e8c4d1',

        border: {
          width: 0,
          radius: 0,

        },
        width: 280,
        tip: { // Now an object instead of a string
          corner: 'leftTop', // We declare our corner within the object using the corner sub-option
          color: '#e8c4d1',
          size: {
            x: 8, // Be careful that the x and y values refer to coordinates on screen, not height or width.
            y : 20 // Depending on which corner your tooltip is at, x and y could mean either height or width!
          }
        }  }
      ,
      show: 'mouseover',
      hide: 'mouseout'
    });
  });
</script>
<script>
  jQ(function(){
    jQ('td.tip_std').qtip({
      content: { text: false },
      position: {
        corner: {
          target: 'topMiddle',
          tooltip: 'bottomMiddle'
        }
      },
      style: {
        name: 'cream',
        border: {
          width: 0,
          radius: 0,
          color: '#99911a'
        },
        width: 250,
        tip: { // Now an object instead of a string
          corner: 'bottomMiddle', // We declare our corner within the object using the corner sub-option
          color: '#99911a',
          size: {
            x: 20, // Be careful that the x and y values refer to coordinates on screen, not height or width.
            y : 8 // Depending on which corner your tooltip is at, x and y could mean either height or width!
          }
        }  }
      ,
      show: 'mouseover',
      hide: 'mouseout'
    });
  });
</script>