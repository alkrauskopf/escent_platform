<div id="db_tools_<%=subject.id%>" >
  <div class="teacher_review_period_box ">
    <div class= "precision_prep_review_panel">
      <div class="teacher_review_header_label">
      Reinitalize Student Plan Markers for Dashboards
      </div>
    </div>
    <div>
      <div class="precision_milestone_panel">
        <div class="precision_prep_review_box">
          <div class="dashboard_marker_text_box">
            This process is generally not necessary, however there may be times when you want to ensure student plan marker
            shown on the dashboards are accurate and up-to-date.  Refreshing these markers may take a while.
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="precision_milestone_panel">
    <div class="precision_prep_review_box">
      <div class="row dashboard_marker_refresh_row">
        Organization Dashboard | <%=subject.name %>
      </div>
      <div class="row subject_student_db_row">
        <div class="col-md-12 subject_student_name">
          <%= render :partial => "/ifa/ifa_dashboard/plan_marker_refresh", :locals=>{:subject=>subject, :entity=>organization} %>
        </div>
      </div>
    </div>
  </div>
  <% if !classrooms.empty? -%>
    <div class="precision_milestone_panel">
      <div class="precision_prep_review_box">
        <div class="row dashboard_marker_refresh_row">
          Classroom Dashboards | <%=subject.name %>
        </div>
        <% classrooms.each do |classroom| -%>
          <div class="row subject_student_db_row">
            <div class="col-md-12 subject_student_name">
              <%= render :partial => "/ifa/ifa_dashboard/plan_marker_refresh", :locals=>{:subject=>subject, :entity=>classroom} %>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  <% end %>
  <% if !students.empty? -%>
    <div class="precision_milestone_panel">
      <div class="precision_prep_review_box">
        <div class="row dashboard_marker_refresh_row">
          Student Dashboards | <%=subject.name %>
        </div>
        <% if false %>
          <div class="row subject_student_db_row">
            <div class="col-md-12 subject_student_name">
              <%= render :partial => "/ifa/ifa_dashboard/plan_marker_refresh_all", :locals=>{:subject=>subject, :entity_list=> students, :entity_class=>'User'} %>
            </div>
          </div>
        <% end %>
        <% students.each do |student| -%>
          <div class="row subject_student_db_row">
            <div class="col-md-12 subject_student_name">
              <%= render :partial => "/ifa/ifa_dashboard/plan_marker_refresh", :locals=>{:subject=>subject, :entity=>student} %>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  <% end %>
</div>