        <colgroup>
          <col style="width:26%">
          <col style="width:16%">
          <col style="width:16%">
          <col style="width:16%">
          <col style="width:26%">
        </colgroup>
        <% @dashboard_header['strat_0'].size.times.with_index do |idx| %>
