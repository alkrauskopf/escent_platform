
<div class="row dashboard_title_1">
  <div class="col-md-2 dashboard_header_title_side">
    <%= (time_ago_in_words(@dashboard_header['taken_time']).sub(/about /, '') + " ago").titleize %>
    <div>
      <%= @dashboard_header['db_type'] %>
    </div>
  </div>
  <div class="col-md-8 dashboard_header_title_center">
    <%= @dashboard_header['row1'] %>
    <%= @dashboard_header['row1_scores'] %>
  </div>
  <div class="col-md-2 dashboard_header_title_side">
    <%= @dashboard_header['row1_right'] %>
  </div>
</div>
<% if !@dashboard_header['table_0'].nil? %>
  <div class="row">
    <div class="col-md-1 dashboard_header_table_side"></div>
    <div class="col-md-10 ">
      <table class="table table-hover table-striped rl_table">
        <tbody>
          <colgroup>
            <col style="width:23%">
            <col style="width:13%">
            <col style="width:13%">
            <col style="width:13%">
            <col style="width:13%">
            <col style="width:25%">
          </colgroup>
          <% @dashboard_header['table_0'].size.times.with_index do |idx| %>
            <% if idx == 0 %>
              <tr >
                <td class="dashboard_header_table_label">
                  <%= @dashboard_header['table_0'][idx].html_safe %>
                </td>
                <td class="dashboard_header_table_header">
                  <%= @dashboard_header['table_1'][idx].html_safe %>
                </td>
                <td class="dashboard_header_table_header">
                  <%= @dashboard_header['table_2'][idx].html_safe %>
                </td>
                <td class="dashboard_header_table_header">
                  <%= @dashboard_header['table_3'][idx].html_safe %>
                </td>
                <td class="dashboard_header_table_header">
                  <%= @dashboard_header['table_4'][idx].html_safe %>
                </td>
                <td class="dashboard_header_table_note">
                  <%= @dashboard_header['table_5'][idx].html_safe %>
                </td>
              </tr>
            <% else %>
              <tr >
                <td class="dashboard_header_table_label">
                  <i class=<%= @dashboard_header['table_glyph'][idx] %>></i>&nbsp;&nbsp;<%= @dashboard_header['table_0'][idx] %>
                </td>
                <td class="dashboard_header_table_stat">
                  <%= @dashboard_header['table_1'][idx] %>
                </td>
                <td class="dashboard_header_table_stat">
                  <%= @dashboard_header['table_2'][idx] %>
                </td>
                <td class="dashboard_header_table_stat">
                  <%= @dashboard_header['table_3'][idx] %>
                </td>
                <td class="dashboard_header_table_stat">
                  <%= @dashboard_header['table_4'][idx] %>
                </td>
                <td class="dashboard_header_table_note">
                  <%= @dashboard_header['table_5'][idx] %>
                </td>
              </tr>
            <% end %>
          <% end %>
        </tbody>
      </table>
    </div>
    <div class="col-md-1 dashboard_header_table_side"></div>
  </div>
<% end %>
<% if !@dashboard_header['strat_0'].nil? %>
