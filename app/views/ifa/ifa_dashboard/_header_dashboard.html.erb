                  </td>
                  <td class="dashboard_header_table_header">
                    <%= @dashboard_header['strat_1'][idx].html_safe %>
                  </td>
                  <td class="dashboard_header_table_header">
                    <%= @dashboard_header['strat_2'][idx].html_safe %>
                  </td>
                  <td class="dashboard_header_table_header">
                    <%= @dashboard_header['strat_3'][idx].html_safe %>
                  </td>
                  <td class="dashboard_header_table_note">
                    <%= @dashboard_header['strat_4'][idx].html_safe %>
                  </td>
                </tr>
            <% else %>
                <tr >
                  <td class="dashboard_header_table_label">
                    <%= @dashboard_header['strat_0'][idx] %>
                  </td>
                  <td class="dashboard_header_table_stat">
                    <%= @dashboard_header['strat_1'][idx] %>
                  </td>
                  <td class="dashboard_header_table_stat">
                    <%= @dashboard_header['strat_2'][idx] %>
                  </td>
                  <td class="dashboard_header_table_stat">
                    <%= @dashboard_header['strat_3'][idx] %>
                  </td>
                  <td class="dashboard_header_table_note">
                    <%= @dashboard_header['strat_4'][idx].html_safe %>
                  </td>
                </tr>
            <% end %>
        <% end %>
        </tbody>
      </table>
    </div>
    <div class="col-md-1 dashboard_header_table_side"></div>
  </div>
<% end %>



