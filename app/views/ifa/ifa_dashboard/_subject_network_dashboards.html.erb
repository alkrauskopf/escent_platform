<div id="network_dbs_<%=subject.id%>" >
  <div class="teacher_review_period_box ">
    <div class= "precision_prep_review_panel">
      <div class="teacher_review_header_label">
      <%= pluralize(schools.size, @org_dashboards['name'] + ' School') %>
      </div>
    </div>
  </div>
  <% if !schools.empty? -%>
    <div class="precision_milestone_panel">
      <% schools.each do |school| -%>
        <%= render :partial => "/ifa/ifa_dashboard/subject_org_dashboards", :locals=>{:dashboards => @org_dashboards[school.id], :organization=>school, :subject => subject, :view_code=>school.id.to_s} %>
      <% end %>
    </div>
  <% end %>
</div>

