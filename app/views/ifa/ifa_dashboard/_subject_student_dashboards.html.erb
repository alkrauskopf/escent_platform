<div id="student_dbs_<%=subject.id%>" >
  <div class="teacher_review_period_box ">
    <div class= "precision_prep_review_panel">
      <div class="teacher_review_header_label">
      <%= pluralize(students.size, @student_dashboards['name'] + ' Student') %>
      </div>
    </div>
  </div>
  <% if !students.empty? -%>
    <div class="precision_milestone_panel">
      <div class="precision_prep_review_box">
        <% students.each do |student| -%>
          <div class="row subject_student_db_row">
            <div class="col-md-12 subject_student_name">
              <table id="subj_dbs_<%=student.id %>">
                <tbody>
                  <% student_label = student.last_name_first.upcase + ' (' + @student_dashboards[student.id].size.to_s + ')' %>
                  <tr>
                    <td class="resource_levels_close">
                      <%= link_to '', ifa_dashboards_entity_subject_path(:organization_id => @current_organization, :entity_class => "User", :act_subject_id => subject.id, :entity_id => student) %>
                      <%= student_label %>
                      <span></span>
                    </td>
                  </tr>
                </tbody>
              </table>
              <script type="text/javascript" charset="utf-8">
                jQ(document).ready(function() {
                  jQ('#subj_dbs_<%=student.id %> .resource_levels_close').toggleResourceLevels();
                  jQ('#subj_dbs_<%=student.id %> .resource_levels_open').toggleResourceLevels();
                });
              </script>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  <% end %>
</div>

