<div id="review_plans_<%=subject.id%>" >
  <div class="row">
    <div class="ifa_panel_title"></div>
  </div>
  <div class="teacher_review_period_box ">
    <div class= "precision_prep_slay_panel">
      <div class="teacher_review_header_label">
      <%= pluralize(students.size, subject.name + ' Student') %>
      </div>
    </div>
  </div>
  <div class="precision_milestone_panel">
    <% students.each do |student| -%>
      <%= render :partial => '/ifa/ifa_plan/teacher_review_plan', :locals=>{:student_plan => nil, :student => student, :subject => subject, :show => 'No'} %>
    <% end %>
  </div>
</div>