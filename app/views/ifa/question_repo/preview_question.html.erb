<div id="ifa_question_preview" class="preview_box">
  <div class= 'row panel_title_blue'>
    <div class="col-md-2"></div>
    <div class="col-md-8">
      Question PreView
    </div>
    <div class="col-md-2"></div>
  </div>
  <% if !@current_question.nil? %>
  <div class= 'row'>
    <div class="col-md-12">
      <div class="row">
        <div class="col-md-2"></div>
        <div class="col-md-8 ">
          <hr>
        </div>
        <div class="col-md-2"></div>
      </div>
      <%= render(:partial => "ifa/submission/view_question_in_assessment" , :locals => {:quest => @current_question, :num => 1, :review=> false, :superstudent=>true, :strategies => []}) %>
    </div>
  </div>
  <% else %>
    <div class= 'row panel_title_blue'>
      <div class="col-md-12">
        No Question
      </div>
    </div>
  <% end %>
  <hr>
</div>

