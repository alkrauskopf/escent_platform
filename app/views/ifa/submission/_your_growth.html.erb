<div class="row ifa_panel_title">
  <%= @current_user.full_name %> Assessment History, <%= pluralize(@dashboard_subjects.size, ' Subject') %>
</div>

<div class="row assess_to_take">
  <div class="col-md-1"></div>
  <div class="col-md-10">
    <div id="assign_resources_panel">
      <table id="subject_levels">
        <tbody>
        <% @dashboard_subjects.each do |sub| -%>
            <tr>
              <td class="authorization_levels_close">
                <%= link_to '', ifa_dashboard_growth_path(:organization_id => @current_organization, :classroom_id => @classroom, :topic_id => @topic, :group => "student", :subject_id => sub.id, :id => @current_user) %>
                <strong><%= sub.name.titleize %></strong>
                <span></span>
              </td>
            </tr>
        <% end -%>
        </tbody>
      </table>
      <br/>
    </div>
  </div>
  <div class="col-md-1"></div>
</div>

  <script type="text/javascript" charset="utf-8">
    jQ(document).ready(function() {
      jQ('#subject_levels .authorization_levels_close').toggleAuthorizationLevels();
      jQ('#subject_levels .authorization_levels_open').toggleAuthorizationLevels();
    });
  </script>