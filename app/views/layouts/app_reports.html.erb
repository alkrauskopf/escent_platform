<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		 <title>Escent Partners, LLC, <%= @current_organization.name %></title>
		<%= stylesheet_link_tag 'screen' %>
		<%= stylesheet_link_tag iwing_stylesheet_url(:organization_id => @current_organization, :format => "css") %>

		<%= stylesheet_link_tag "public_info", :media => "screen" %>
		<%= stylesheet_link_tag "fsn/jquery-ui-1.7.1.custom.css", :media => "screen" %>
		<%= stylesheet_link_tag "master/print", :media => "print" %>
		<%= stylesheet_link_tag "jquery.ui.potato.menu.css" %>
		<%= javascript_include_tag 'prototype' %>
		<%= javascript_include_tag 'jquery-1.3.2.min.js' %>
		<script type="text/javascript" charset="utf-8">
			jQ = jQuery.noConflict();

		</script>
		<%= javascript_include_tag 'swfobject.js' %>
		<%= javascript_include_tag "jquery.qtip-1.0.0.min.js" %>
		<%= javascript_include_tag 'jquery-ui-1.7.1.custom.min.js' %>
		<%= javascript_include_tag 'jquery.tablesorter.min.js' %>
		<%= javascript_include_tag 'jquery.MetaData.js' %>
		<%= javascript_include_tag 'jquery.rating.js' %>
		<%= javascript_include_tag 'jquery.validate.min.js' %>
		<%= javascript_include_tag 'jquery.corner.js' %>
		<%= javascript_include_tag 'jquery.bgiframe.js' %>
		<%= javascript_include_tag 'jquery.ajaxQueue.js' %>
		<%= javascript_include_tag 'jquery.autocomplete.min.js' %>
		<%= javascript_include_tag '/player/swfobject.js' %>
		<%= javascript_include_tag 'application.js' %>
		<%= javascript_include_tag 'wforms_core.js' %>
		<%= javascript_include_tag 'wforms_sync.js' %>
		<%= javascript_include_tag 'wforms_validation.js' %>
		<%= javascript_include_tag 'wforms_switch.js' %>
		<%= javascript_include_tag 'wforms_repeat_rails.js' %>
		<%#= javascript_include_tag 'tiny_mce/tiny_mce.js' %>
		<%= javascript_include_tag 'jquery.tools.min.js' %>
		<%= javascript_include_tag 'jquery.ui.potato.menu.js' %>
		<style type="text/css">
			a.continue {
				text-transform: uppercase;
				text-decoration: none;
				color: #ffffff;
				display: block;
				text-align: right;
				font-weight: bold;
				font-size: 12px;
				font-family: Helvetica, Arial;
			}
			a.continue:hover {
				text-decoration: underline;
			}
			#main_content {
                width: 1200px;
                padding: 0 30px 0 30px;
				background: transparent;
			}
			#mainContent {
				background: #FFFFFF;

            } #main_content #main_header {
                margin-top: 0px;
                width: 1200px;
                height: 242px;
                background: url(<%= asset_path('app_blog_header.jpg') %>) top left no-repeat;
                position: relative;
            } #main_content #main_header #masthead{
                margin-top: 30px;
				width: 1100px;
                height: 93px;
                position: absolute;
            } #main_content #main_header div#welcome_msg {
                position: absolute;
                top: 60px;
                left: 0px;
             } #main_content #main_header div#login_bar {
                position: absolute;
                top: 10px;
                width: 100%; 
             } #main_content #main_header div#header_bar {
                position: absolute;
                top: 180px;
                width: 100%; 
            } #main_content div#report_box {
                margin-top: 0px;
                width: 1195px;
                border:#000000;
                border-width:2px;
                position: relative;
            } #report_bottom {
                width: 1200px;
                padding: 0 30px 0 30px;
				background: transparent;
			}
		</style>
		<script type="text/javascript" charset="utf-8">
			jQ(document).ready(function() {
				jQ.ajaxSetup({
					'beforeSend' : function(xhr) {
						xhr.setRequestHeader("Accept", "text/javascript")
					}
				});
				jQ("#loading").ajaxStart(function() {
					jQ(this).show()
				}).ajaxStop(function() {
					jQ(this).hide()
				});
				jQ('.fsn-button').addUIStateClass();
			})
		</script>
	</head>
	<body class="iwing_page_background" >

			<div id="home_body">
 				<div id="main_content">
					<div id="main_header">
						<div id="masthead" style="font-size:30px; padding-left:50px; padding-right:0px; line-height:1.8em; font-weight:bold; color:#dbe4ed; font-family:Slicker,Arial;">
							<%= @provider.name %>
							<br/>
							<%= @provider.app_settings(@app).alt_name %>&nbsp;Reports & Analysis
						</div>
				    <div id="welcome_msg">
				        <h1 class="hidden">Welcome to Escent Partners, Education Transformation, Teacher Effectiveness, <%= @provider.name %></h1>
				    </div>
				
				   	<div id="header_bar">
						<div id="header_main_ep">
						    <%= render(:partial => "/shared/header_bar_reports") %>
					   	</div>
				   	</div>
				</div>

					<div id="report_box" >
						<%= yield %>
					</div>
				</div>
				<div style="clear:both">
					&nbsp;
				</div>
				<div class="bottom"></div>
			</div>
			<div id="report_bottom">
			<div class="iwing_footer">
				<div class="iwing_header_and_footer_bars">
					<%= render(:partial => "/shared/footer") %>
				</div>
			</div>
        <%= render(:partial => '/layouts/footer_tos_pp') %>
		</div>
		<div id="loading" style="display:none;">
			<%= image_tag asset_path('ajax-loader.gif') %>
		</div>
		<script type="text/javascript">
			/*
			 var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
			 document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
			 */
		</script>
		<script type="text/javascript">
			try {
				var pageTracker = _gat._getTracker("UA-10167834-1");
				pageTracker._trackPageview();
			} catch (err) {
			}
		</script>
	</body>
</html>
