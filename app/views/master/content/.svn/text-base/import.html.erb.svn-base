<div class="formBox" style="height:100px;">
    <h2>Import Content Upload</h2>
    <% form_tag({:action => :import}, {:multipart => true} ) do %>
    Select CSV File <%= file_field 'upload', 'datafile' %>
    <%= submit_tag "Upload" %>
    <% end %>
</div>
    <%= @status %>
    <% unless @contents.empty? %>
    <table style="background-color:#FFFFFF">
        <thead>
            <tr>
                <th>
                    Title
                </th>
                <th>
                	Caption
                </th>
				<th>
					Organization
				</th>
				<th>Series</th>
				<th>Description</th>
				<th>Source Name</th>
				<th>Source URL</th>
				<th>Star performer</th>																
				<th>Creator Name</th>
				<th>Start Date</th>
				<th>End Date</th>								
				<th>User ID</th>												
				<th>Source File</th>		
				<td>Type ID</td>																		
            </tr>
        </thead>
        <tbody>
            <% @contents.each do |content| -%>
            <tr>
                <td><%= content.title %>
                </td>
                <td><%= content.caption %>
                </td>				
                <td><%= content.organization_id %>
                </td>				
                <td><%= content.series %>
                </td>								
                <td><%= content.description %>
                </td>				
                <td><%= content.source_name %>
                </td>	
                <td><%= content.source_url %>
                </td>	
                <td><%= content.star_performer %>
                </td>	
                <td><%= content.creator_name %>
                </td>	
                <td><%= content.publish_start_date %>
                </td>	
                <td><%= content.publish_end_date %>
                </td>	
                <td><%= content.user_id %>
                </td>	
                <td><%= content.source_file_file_name %>
                </td>	
                <td><%= content.content_object_type_id %>
                </td>																																	
            </tr>
            <% end -%>
        </tbody>
    </table>
    <% end %>

