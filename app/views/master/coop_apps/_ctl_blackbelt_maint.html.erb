<div id="ctl_blackbelt_maint">

	<% if @current_user.app_superuser?(app) -%>
	
		<br/>
		<% TltSession.of_video.collect{|s| s.content}.compact.uniq.each do |vid|%>
			<%= render :partial => "master/coop_apps/ctl_blackbelt_video", :locals => {:app => app, :video=>vid}  %>		
		<% end -%>

	<% else -%>
		<span style="font-size:12px; color: #2061a5;font-weight:normal">
			<center><br/>* * * Not Authorized * * *</center>
		</span>		
	<% end -%>
<br/>

</div>

