
	<% if @current_organization.projects_featured  %>
		<%= link_to "Our Projects", :controller => "fsn", :action => :index, :organization_id => @current_organization, :blog_id => @current_organization.projects_featured.public_id %>	
	<% else -%>
		Our Projects
	<% end -%>
	<ul>
		<% @current_organization.projects.each do |blog| -%>
			<li style="width: 330px;"><%= link_to blog.title, :controller => "fsn", :action => :index, :organization_id => @current_organization, :blog_id => blog.public_id%></li>
		<% end -%>
	</ul>
