
	<a href="#" >Apps</a>
	<% if @current_organization.can_be_displayed?(@current_user)-%>
		<ul>
			<% @current_organization.enabled_apps.sort_by{|a| a.name}.each do |app| %>
				<% if @current_user.app_authorized?(app, @current_organization) -%>
					<% app_label = (app.beta? ? "<small>(beta) </small>" + @current_organization.provider_app_name(app) : @current_organization.provider_app_name(app)) -%>
					<li style="width: 230px;"><%= link_to app_label, :controller => app.app_link[0], :action => app.app_link[1], :organization_id => @current_organization,:user_id => @current_user, :teacher_id => @current_user   %></li>
				<% end %>
			<% end %>
		</ul>
	<% end -%>
