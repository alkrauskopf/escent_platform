<div class="module_header_edit">
<% unless @current_user.nil? %>
  <% if @current_user.administrator_of?(@current_organization) || @current_user.superuser? -%>
    <span class="normal_text">
      <%= link_to 'Edit', admin_path(:organization_id => @current_organization) %>
    </span>
  <% end %>
<% end %>
</div>                
