/* set style about font */
/* Titles */
.title_large_red { font-family: Arial, Helvetica, sans-serif; font-size: 18px; font-weight: bold; color: #C83A14; line-height:20px;	}
.title_large_blue { font-family: Arial, Helvetica, sans-serif; font-size: 18px; font-weight: bold; color: #2971BD; line-height:20px; }
.title_large_green { font-family: Arial, Helvetica, sans-serif; font-size: 18px; font-weight: bold; color: #AABA0F; line-height:20px; }
.title_large_orange { font-family: Arial, Helvetica, sans-serif; font-size: 18px; font-weight: bold; color: #E9A713; }
.title_red { font-family: Arial, Helvetica, sans-serif; font-size: 12px; font-weight: bold; color: #C83A14; }
.title_red_nobold { font-family: Arial, Helvetica, sans-serif; font-size: 12px; font-weight: normal; color: #C83A14; }
.title_blue { font-family: Arial, Helvetica, sans-serif; font-size: 12px; font-weight: bold; color: #2971BD; }
.title_green { font-family: Arial, Helvetica, sans-serif; font-size: 12px; font-weight: bold; color: #AABA0F; }
.title_orange { font-family: Arial, Helvetica, sans-serif; font-size: 12px; font-weight: bold; color: #E9A713; }
.text_dark_blue { font-family: Arial, Helvetica, sans-serif; font-size: 12px; color: #11118A; }

/* Admin Roles */
#roles_panel input { font-size:11px; }
/* Admin Associates */
#associates_panel input { font-size:11px; }
/* Admin Content */
#result-box #content_panel td input { font-size:11px; }
#content_panel .search-box input { font-size: 12px; }
/* admin Our Family */
.authorization_levels_open a,
.authorization_levels_close a { text-decoration: none; }

.resource_levels_open a,
.resource_levels_close a { text-decoration: none; }
.ifa_levels_open a,
.ifa_levels_close a { text-decoration: none; }
.view_levels_open a,
.view_levels_close a { text-decoration: underline; }

/* App Blog Box */
.appblog_box { font-family:Verdana, Arial, Helvetica, sans-serif; color:#636363; font-size:11px; }
.appblog_box a { color:#2176ad; }
.appblog_box .instructions { color:#2176ad; font-size:9px; font-weight:bold; }
.appblog_box .title_large_blue { text-align:left;  font-size:14px; }


/* Member Profile */
.mempro_box { font-family:Verdana, Arial, Helvetica, sans-serif; color:#636363; font-size:11px; }
.mempro_box a { color:#2176ad; }
.mempro_box .instructions { color:#2176ad; font-size:9px; font-weight:bold; }
.mempro_box .title_large_blue { text-align:left;  font-size:14px; }

/* some font style about form */
.FormRowTitle { font-family: Verdana,Arial,Helvetica,sans-serif; font-size:12px; text-align: right; }
.FormRowData{ text-align:left; }		
.FormSectionTitle { font-family: Verdana,Arial,Helvetica,sans-serif; font-size:12px; font-weight:bold; text-align:left; }
.FormRow .small_notice { font-size:85%; font-style:italic; }

/* make a admin Organization Account tag section selector much more clean */
.section_selector a { text-decoration: none; }
.section_selector { line-height: 32px; }

/* admin iWing css */
.topics_panel_title h4 { font-weight: bold; font-size: 13px; }
#FormBoxWarp h4 { font: bold 12px/12px Verdana,Arial,Helvetica,sans-serif; text-align:left; }

/* font style about front site */
#iwcontent { font-weight:normal; }
#staticorg { font-weight:normal; }
#leftpane { font-size:12px; font-weight:normal; }
#leftpane_off { font-size:12px; font-weight:normal; }
#rightorgpane { font-size:12px; font-weight:normal; }
.module { font-weight:normal; }
.module2 { font-weight:normal; }
#rightpane_off { text-align:center; }
#rightpane { text-align:center; }
.module_header { font-family:Verdana, Arial, Helvetica, sans-serif; font-size:14px; font-weight:bold; }
#control_panel #description_for_vote #votes { text-align: right; }
#root_discussion_form { font-size: 14px; }
#share_content_form { font-size: 11px; }

#share_pilot_feedback_form { font-size: 11px; }

.root_discussion_trigger { cursor:pointer; }
.root_discussion_trigger:hover { text-decoration:underline; }
.discussion_thread_header { font-family:Verdana, Arial, Helvetica, sans-serif; font-size:14px; line-height: 35px; font-weight:bold; }
.iwing_module_title { font-size: 12px; text-align: left; }
.iwing_module_title abbr { font-weight: normal; }
.iwing_module_title em { font-weight: normal; }
.module_header .root_discussion_trigger { font-size: 12px; }
.discussion_thread_header .iwing_module_title div { font-size: 12px; font-weight: normal; line-height: 12px; }
.reply_discussion_trigger { text-align: right; font-size: 11px; font-weight: normal; }
.discussion_thread_header .iwing_module_title { text-indent: 40px; }
.discussion_thread_header .iwing_discussion_description,
.discussion_replys, .discussion_reply_comment { line-height: 20px; }
.results { text-align: left; }
.discussion_replys, .iwing_discussion_description { font-size: 12px; font-weight: normal; line-height: 26px; }
.discusssion_by_same_author_info,
.discussion_reply_info { color: #b3b3b3; }
.discusssion_by_same_author_info { text-indent: 60px; line-height: 20px; }
.discussion_reply_info span.change_status { font-weight: normal; }
.discussion_root_login_notice,
.discussion_reply_login_notice { text-align:left; color:#AE3119; font-size: 12px; font-weight: normal; }
.discussion_warp span { color: #b1b1b1; }
.discusssion_by_same_author_info span,
.discussion_reply_info span { font-weight: bold; color: #b1b1b1; }
.discussion_reply_info span.report_successful { font-weight: normal; }
.discussion_reply_form { line-height: 22px; }
.share_content_form_title,
.discussion_root_form_title { font-weight: bold; font-size: 13px; color: #333; line-height: 26px; }
.discussion_reply_form_title { font-size: 12px; color: #333; }
.discussion_reply_form_notice,
.share_content_form_notice { font-size: 12px; font-weight: normal; color: #b3b3b3; }
#share_content a { line-height: 24px; text-decoration: none; }
.share_content_form_title,
.share_discussion_form_title { text-align: left; font-size: 12px; text-indent: 26px; }

.share_pilot_feedback_form_title { font-weight: normal; font-size: 13px; color: #C83A14; line-height: 26px; }
.share_pilot_feedback_form_notice { font-size: 12px; font-weight: normal; color: #b3b3b3; }
#share_pilot_feedback a { line-height: 24px; text-decoration: none; }
.share_pilot_feedback_form_title { text-align: center; font-size: 13px;  }

.share_discussion_form_title { font-weight: bold; }
.share_content_form p, .share_discussion_form p { padding-left: 26px; }
.share_content_form p label, .share_discussion_form p label { text-align: right; }
.share_content_form_notice, .share_discussion_form_notice { text-align: left; }

.share_pilot_feedback_form p { padding-left: 26px; }
.share_pilot_feedback_form p label { text-align: right; }
.share_pilot_feedback_form_notice { text-align: left; }

.more_more_details { text-decoration: none; }
.more_details { text-align: left; }

